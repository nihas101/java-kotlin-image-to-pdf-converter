<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.nihas101.imageToPdfConverter.gui.controller.DirectoryContentDisplayController">
   <ListView fx:id="imageListView" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"
             onMouseClicked="#displayListCell" orientation="HORIZONTAL" prefHeight="200.0" prefWidth="700.0"
             AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
             AnchorPane.topAnchor="0.0" HBox.hgrow="ALWAYS"/>
   <Button fx:id="buildButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" onAction="#buildPDF"
           text="Build"/>
</HBox>
